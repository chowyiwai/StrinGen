<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>


<fx:root type="HBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TextField promptText="Enter module code..." styleClass="enterBox" stylesheets="@theme.css" />
      <ComboBox prefWidth="150.0" promptText="Grade" styleClass="comboBox" stylesheets="@theme.css" />
      <Button mnemonicParsing="false" onAction="#addNewEntry" stylesheets="@theme.css" text="add OR" />
   </children>
</fx:root>
