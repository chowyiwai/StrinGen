<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root maxHeight="1.7976931348623157E308" type="VBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox fx:id="deleteCohortButtonPlaceholder">
         <children>
            <Button fx:id="deleteCohortButton" alignment="CENTER_RIGHT" mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" onAction="#deleteCohort" stylesheets="@theme.css" text="DELETE COHORT">
               <font>
                  <Font size="5.0" />
               </font>
               <padding>
                  <Insets bottom="-3.0" />
               </padding>
            </Button>
         </children>
      </HBox>
      <ListView fx:id="entryListView" maxHeight="1.7976931348623157E308" stylesheets="@theme.css" VBox.vgrow="ALWAYS" />
   </children>
</fx:root>
