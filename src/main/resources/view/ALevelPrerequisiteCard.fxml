<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>

<fx:root stylesheets="@theme.css" type="HBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ComboBox fx:id="subjectField" prefWidth="150.0" promptText="Subject Name" styleClass="comboBox" />
      <TextField fx:id="gradeField" prefHeight="36.0" promptText="Enter minimum grade..." styleClass="enterBox" stylesheets="@theme.css" />
      <Button fx:id="orButton" mnemonicParsing="false" onAction="#addNewEntry" prefHeight="36.0" stylesheets="@theme.css" text="add OR" />
   </children>
</fx:root>
