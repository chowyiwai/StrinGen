<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>

<fx:root maxHeight="-Infinity" maxWidth="-Infinity" stylesheets="@theme.css" type="HBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TextField fx:id="courseCodeField" prefHeight="27.0" promptText="Enter course code..." styleClass="enterBox" HBox.hgrow="ALWAYS" />
      <Button fx:id="orButton" mnemonicParsing="false" onAction="#addNewEntry" prefHeight="27.0" stylesheets="@theme.css" text="add OR" />
   </children>
</fx:root>
